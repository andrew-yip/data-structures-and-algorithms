package HW08BinarySearchTrees;

public class MorseNode {
	
	public String morse;
	public String letter;
	public MorseNode left; // if it is a period
	public MorseNode right; // if it is a dash

	
	public MorseNode (String letter, String morse) {
		this.letter = letter;
		this.morse = morse;
		left = null;
		right = null;
	}

}
